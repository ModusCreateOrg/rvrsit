#!/bin/sh
if test -e "./sdk/command/sencha"
then
    case $OSTYPE in
        darwin*) ./sdk/command/sencha $*;;
        linux*)  ./sdk/command/sencha $*;;
    esac
else
    if test -e "./command/sencha"
    then
        case $OSTYPE in
            darwin*) ./command/sencha $*;;
            linux*)  ./command/sencha $*;;
        esac
    else
        DIR=$(dirname $(which $0))"/../command"
        jsdb -path $DIR $DIR/sencha.js $*
    fi
fi
